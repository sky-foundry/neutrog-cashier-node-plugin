{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": [true],
    "quotemark": [true, "single", "avoid-escape"],
    "semicolon": [true, "never"],
    "arrow-parens": [true, "ban-single-arg-parens"],
    "no-console": false,
    "interface-name": false
  },
  "rulesDirectory": []
}
